    "import pickle\n",
    "\n",
    "#a python object (dictionary in this case)\n",
    "data = {\"name\" : \"Govind\", \"age\" : 21, \"occupation\" : \"student\"}\n",
    "\n",
    "with open (\"data.pkl\", \"wb\") as file:\n",
    "    pickle.dump(data, file)\n",
    "\n",
    "print(\"The data has been serialized and saves to data.pkl\")"
